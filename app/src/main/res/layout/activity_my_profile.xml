<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".UI.MyProfile">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="@string/my_profile"
        android:textColor="@color/colorText"
        android:textSize="@dimen/_22sdp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linear1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/textView3"
        >
        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_marginStart="48dp"
            android:layout_gravity="center"
            android:src="@drawable/ic_baseline_person_24"
            />
        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Name"
            android:layout_marginStart="10dp"
            android:textColor="@color/text"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            />

        <TextView
            android:id="@+id/yourName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="your name"
            android:textColor="#D37AE3"
            android:layout_marginStart="30dp"
            android:layout_gravity="center"
            android:textSize="@dimen/_14sdp"
            android:textStyle="bold|italic"
            />
    </LinearLayout>

 <LinearLayout
     android:id="@+id/linear2"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:orientation="horizontal"
     android:layout_marginTop="30dp"
     app:layout_constraintTop_toBottomOf="@id/linear1">

     <ImageView
         android:id="@+id/image4"
         android:layout_width="@dimen/_30sdp"
         android:layout_height="@dimen/_25sdp"
         android:layout_marginStart="48dp"
         android:layout_gravity="center"
         android:src="@drawable/ic_baseline_phone_24"
         />
     <TextView
         android:id="@+id/textView6"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_gravity="center"
         android:text="Mobile"
         android:layout_marginStart="10dp"
         android:textColor="@color/text"
         android:textSize="@dimen/_15sdp"
         android:textStyle="bold|italic"
         />

     <TextView
         android:id="@+id/yourPhone"
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_gravity="center"
         android:text="your phone"
         android:layout_marginStart="30dp"
         android:textColor="#D37AE3"
         android:textSize="@dimen/_14sdp"
         android:textStyle="bold|italic"
         />
 </LinearLayout>

    <LinearLayout
        android:id="@+id/linear3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/linear2"
        >

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_gravity="center"
            android:layout_marginStart="48dp"
            android:src="@drawable/ic_baseline_email_24"
            />
        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/email"
            android:layout_marginStart="10dp"
            android:textColor="@color/text"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold|italic"
            />

        <TextView
            android:id="@+id/yourEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="your Email"
            android:textColor="#D37AE3"
            android:textSize="@dimen/_14sdp"
            android:layout_marginStart="30dp"
            android:layout_gravity="center"
            android:textStyle="bold|italic"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/linear3">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_25sdp"
           android:layout_gravity="center"
            android:layout_marginStart="48dp"
            android:src="@drawable/ic_baseline_location_on_24"
            />
        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
android:layout_gravity="center"            android:text="@string/address"
            android:textColor="@color/text"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold|italic"
            />

        <TextView
            android:id="@+id/yourAddress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/your_address"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:textColor="#D37AE3"
            android:textSize="@dimen/_14sdp"
            android:textStyle="bold|italic"
            />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/linear4">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_25sdp"
android:layout_gravity="center"            android:layout_marginStart="48dp"
            android:src="@drawable/ic_baseline_date_range_24"
            />
        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
android:layout_gravity="center"            android:text="@string/d_o_b"
            android:textColor="@color/text"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold|italic"
            />
        <TextView
            android:id="@+id/DateBirth"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="your Date Of Birth"
            android:textColor="#D37AE3"
            android:textSize="@dimen/_14sdp"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:textStyle="bold|italic"
            />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/linear6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/linear5">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_25sdp"
android:layout_gravity="center"            android:src="@drawable/ic_gender"
            android:layout_marginStart="48dp"
            />
        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
android:layout_gravity="center"            android:text="@string/gender"
            android:textColor="@color/text"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold|italic"
            />
        <TextView
            android:id="@+id/yourGender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="your Gender"
            android:textColor="#D37AE3"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:textSize="@dimen/_14sdp"
            android:textStyle="bold|italic"
            />
    </LinearLayout>


    <LinearLayout
        android:id="@+id/linear7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp"
        app:layout_constraintTop_toBottomOf="@id/linear6">


        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_25sdp"
android:layout_gravity="center"            android:layout_marginStart="48dp"
            android:src="@drawable/ic_baseline_snippet_folder_24"
            />
        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
android:layout_gravity="center"            android:text="@string/national_id"
            android:textColor="@color/text"
            android:textSize="@dimen/_15sdp"
            android:textStyle="bold|italic"
            />
        <TextView
            android:id="@+id/Yournational_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/your_national_id"
            android:textColor="#D37AE3"
            android:textSize="@dimen/_14sdp"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:textStyle="bold|italic"
            />
    </LinearLayout>

    <Button
        android:id="@+id/edit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/btnshape"
        android:text="@string/edit"
        android:textColor="@color/white"
        android:textSize="20sp"
        android:layout_margin="40dp"
app:layout_constraintTop_toBottomOf="@id/linear7"
        />

</androidx.constraintlayout.widget.ConstraintLayout>